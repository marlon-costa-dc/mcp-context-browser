{
  "context_browser": {
    "collection": "mcp-context-browser",
    "auto_index": {
      "enabled": true,
      "on_session_start": true,
      "incremental": true
    },
    "index_paths": [
      "src/"
    ],
    "exclude_patterns": [
      "target/",
      "tests/fixtures/"
    ],
    "languages": [
      "rust"
    ]
  },

  "search_preferences": {
    "default_limit": 15,
    "prefer_semantic": true,
    "fallback_to_grep": true
  },

  "common_queries": [
    "provider pattern and trait implementation",
    "MCP protocol handlers and tools",
    "vector store and embedding integration",
    "error handling and validation",
    "async service implementation"
  ]
}
